@model DandyDoc.Core.ViewModels.MethodViewModel
@{
	ViewBag.Title = Model.Title;
	Layout = "_Layout.cshtml";
}
<h1>@Model.Title</h1>
@if (null != Model.Summary) {
	Html.RenderPartial("Partials/SummarySection", Model.Summary);
}
@{ Html.RenderPartial("Partials/NamespaceAndAssembly", Model.AssemblyNamespace);}
<h2>Signature</h2>
@if (Model.Definition.Parameters.Count > 0) {
	<h2>Parameters</h2>
	Html.RenderPartial("Partials/ParameterList",Model.CreateParameterViewModels(Model.Definition.Parameters));
}
@if (Model.HasReturn) {
	<h2>Returns</h2>
	Html.RenderPartial("Partials/ReturnSection", Model.CreateReturnViewModel());
}
@{Html.RenderPartial("Partials/DefinitionPageCommonSections", Model);}