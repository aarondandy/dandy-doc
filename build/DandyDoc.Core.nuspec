<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>DandyDoc.Core</id>
		<version>$version$</version>
		<title>Dandy Doc</title>
		<authors>aarondandy</authors>
		<description>A simple documentation generation library to help you make documentation how you want it.</description>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<tags>documentation</tags>
		<projectUrl>http://mediocresoft.com/dandydoc/</projectUrl>
    <dependencies>
      <dependency id="Mono.Cecil" version="$cecilVersion$"/>
    </dependencies>
		<references>
			<reference file="DandyDoc.Core.dll" />
		</references>
	</metadata>
	<files>
		<file src="..\bin\$configuration$\DandyDoc.Core.dll" target="lib\net-40" />
		<file src="..\bin\$configuration$\CodeContracts\DandyDoc.Core.Contracts.dll" target="lib\net-40\CodeContracts" />
		<file src="..\src\DandyDoc.Core\**.cs" target="src" />
	</files>
</package>