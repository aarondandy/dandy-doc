@using DandyDoc.CodeDoc
@using DandyDoc.Web.ServiceStack
@inherits ServiceStack.Razor.ViewPage<CodeDocProperty>
@{
  Layout = "_ApiDocsLayout";
}
<section>
  <h2>Value</h2>
  <dl>
    <dt>Type</dt>
    <dd>@this.ActionLinkFull(Model.ValueType)</dd>
  </dl>

  @if (Model.HasValueDescriptionContents) {
    <div>@this.XmlDocHtml(Model.ValueDescriptionContents)</div>
  }
</section>
@if (Model.HasGetter && DandyDocHelpers.CodeDocAccessorIsWorthDisplaying(Model.Getter)) {
  <section>
    <h2>Getter</h2>
    @this.CodeDocAccessor(Model.Getter)
  </section>
}
@if (Model.HasSetter && DandyDocHelpers.CodeDocAccessorIsWorthDisplaying(Model.Setter)) {
  <section>
    <h2>Setter</h2>
    @this.CodeDocAccessor(Model.Setter)
  </section>
}